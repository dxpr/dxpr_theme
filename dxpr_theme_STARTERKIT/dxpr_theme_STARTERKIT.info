name = DXPR Theme STARTERKIT Example
description = For basic customization just use DXPR Theme and theme settings. Use this STARTERKIT example if you need to build extra css files and templates on top of DXPR Theme Theme.
core = 7.x
version = 7.x-1.0.0-beta1
base theme = dxpr_theme

; Regions
regions[navigation]                                   = 'Navigation'
regions[secondary_header]                             = 'Secondary Header'
regions[slider]                                       = 'Hero Region'
regions[header]                                       = 'Page Title'
regions[highlighted]                                  = 'Highlighted'
regions[help]                                         = 'Help'
regions[content_top]                                  = 'Content Top'
regions[content]                                      = 'Content'
regions[content_bottom]                               = 'Content Bottom'
regions[sidebar_first]                                = 'Sidebar First'
regions[sidebar_second]                               = 'Sidebar Second'
regions[footer]                                       = 'Footer'

settings[jquery_update_jquery_version] = 2.1

;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/dxpr_theme_subtheme.css
