{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "window": true,
    "Drupal": true,
    "drupalSettings": true,
    "drupalTranslations": true,
    "domready": true,
    "jQuery": true,
    "_": true,
    "matchMedia": true,
    "Backbone": true,
    "Modernizr": true,
    "CKEDITOR": true,
    "WOW": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-undef": 2,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    // "no-unused-vars": 0,
    "strict": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-new": 0,
    "no-alert": 0,
    "no-use-before-define": 2,
    "consistent-return": 0,
    "no-constant-condition": 2,
    "no-shadow" : 2
  }
}
